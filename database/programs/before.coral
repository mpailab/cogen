f arg thrm =
  case arg of
    theorem [thrm]
    do
      {-
        Comment 1
      -}
      case thrm of
        forall [boundVariables vars, premises prems, conclusion [concl]] where vars eq thrm
        do
          h [x, y] = concl where h in [equality, equivalence] -- comment 2
            x eq y -- comment 3
          Efgh = x -- cbcd
          Abcd = h
          x_header [equality [a&(plus [p&x1, x2]), b@(plus [x3, x1])]] <- x
        done
      done
  done
