theorem t =
  -- Parse the theorem of inference rule
  if Compiler
    case t of
      -- case of a forall theorem
      "forall" ("boundVariables" v, "premises" p, "conclusion" (c))
        Bounds = v
        Premises = p
        g(l,r) = c where g in {"equality", "equivalence"}
        (From,To) = LeftToRight ? (l,r) : (r,l)
